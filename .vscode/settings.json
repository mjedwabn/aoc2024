{
    "rust-analyzer.linkedProjects": [
        "./Cargo.toml"
    ],
    "rust-analyzer.showUnlinkedFileNotification": false,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.tabSize": 2
    },
    "rust-analyzer.rustfmt.extraArgs": [
        "--config",
        "tab_spaces=2"
    ],
}